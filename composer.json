{
    "name": "sandbox-dev/laravel-userstamps",
    "description": "Laravel user stamps package",
    "license": "MIT",
	"authors": [
        {
            "name": "Sandbox"
		}
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
		"orchestra/testbench": "^6.0 || ^7.0 || ^8.0",
        "larapack/dd": "^1.1",
        "doctrine/dbal": "^3.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sandbox\\UserStamps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandbox\\UserStamps\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sandbox\\UserStamps\\UserStampsServiceProvider"
            ]
        }
    }
}
